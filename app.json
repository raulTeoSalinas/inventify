{
  "expo": {
    "name": "Inventify",
    "slug": "Inventify",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/app/icon.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./src/assets/images/app/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "userInterfaceStyle": "automatic",
      "supportsTablet": true,
      "bundleIdentifier": "com.lesnoy.Inventify"
    },
    "android": {
      "userInterfaceStyle": "automatic",
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/app/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.lesnoy.Inventify"
    },
    "web": {
      "favicon": "./src/assets/images/app/favicon.png"
    },
    "plugins": [
      "expo-font"
    ],
    "extra": {
      "eas": {
        "projectId": "03aa0b48-666f-4431-9e99-249eb48035a5"
      }
    }
  }
}