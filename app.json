{
  "expo": {
    "name": "Inventify",
    "slug": "Inventify",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/app/icon.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./src/assets/images/app/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#05101d"
    },
    "ios": {
      "userInterfaceStyle": "automatic",
      "supportsTablet": true,
      "bundleIdentifier": "com.lesnoy.Inventify",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "userInterfaceStyle": "automatic",
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/app/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.lesnoy.Inventify",
      "permissions": [
        "android.permission.RECORD_AUDIO",
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "favicon": "./src/assets/images/app/favicon.png"
    },
    "plugins": [
      "expo-font",
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to set an image for your company."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "3903d796-d73b-4e7f-a0a2-2c9f0a6004df"
      }
    },
    "owner": "lesnoy",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/3903d796-d73b-4e7f-a0a2-2c9f0a6004df"
    }
  }
}
